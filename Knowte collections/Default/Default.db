mponent footer\n\nInstalar Bootstrap\nen angular.json\nnpm install bootstrap jquery @popperjs/core\nnpm install bootstrap --save\nnpm install jquery --save\nnpm install popper.js --save\n\nInstalar mapas \nnpm install @agm/core --save --force\nen src/app/app.module.ts\nagregar el api key\n\nimport { AppComponent } from './app.component';\n\n@NgModule({\n  imports: [\n    BrowserModule,\n    AgmCoreModule.forRoot({\n      apiKey: ''\n    })\n\n\n\n","isSelected":false,"closedTasksCount":0,"totalTasksCount":0,"isTrashed":false,"trashedDate":0,"meta":{"revision":50,"created":1637679660533,"version":0,"updated":1639278919808},"$loki":32,"displayModificationDate":"7 months ago","displayExactModificationDate":"Saturday, December 11, 2021"},{"title":"Data Tables","notebookId":"-jVpiLpNAE15T-_dWZ-jB","id":"CBVLH4Qhg6ZGwALB_0kjp","isMarked":false,"creationDate":1637771297759,"modificationDate":1637771504117,"text":"npm install jquery --save\nnpm install datatables.net --save\nnpm install datatables.net-dt --save\nnpm install angular-datatables --save\nnpm install @types/jquery --save-dev\nnpm install @types/datatables.net --save-dev\nAgregar \na angular.json\n \"styles\": [\n              \"node_modules/datatables.net-dt/css/jquery.dataTables.css\"\n            ],\n            \"scripts\": [\n              \"node_modules/jquery/dist/jquery.js\",\n              \"node_modules/datatables.net/js/jquery.dataTables.js\"\n            ],\nImportar en el module.ts\n","isSelected":false,"closedTasksCount":0,"totalTasksCount":0,"isTrashed":false,"trashedDate":0,"meta":{"revision":5,"created":1637771297764,"version":0,"updated":1637771504117},"$loki":33,"displayModificationDate":"7 months ago","displayExactModificationDate":"Wednesday, November 24, 2021"},{"title":"Modificaciones","notebookId":"_6b6sPQdPhoFZxV3_llPe","id":"cmUH6nIwOMM2Cahb7kwOL","isMarked":false,"creationDate":1637932692192,"modificationDate":1637932758556,"text":"/opt/odoo/odoo/addons/hr_payroll/models/hr_payslip.py\n